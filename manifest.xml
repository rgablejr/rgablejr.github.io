<VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
  <Hosts>
    <Host xsi:type="Workbook">
      <FunctionFile resid="Commands.Url" />
      
      <DesktopFormFactor>
        <ExtensionPoint xsi:type="PrimaryCommandSurface">
          <CustomTab id="MyCustomTab">
            <Group id="MyGroup">
              <Label resid="GroupLabel" />
              <Control xsi:type="Button" id="MyButton">
                <Label resid="ButtonLabel" />
                <Supertip>
                  <Title resid="ButtonLabel" />
                  <Description resid="ButtonTip" />
                </Supertip>
                <Icon>
                  <bt:Image size="16" resid="Icon16" />
                  <bt:Image size="32" resid="Icon32" />
                  <bt:Image size="80" resid="Icon80" />
                </Icon>
                <Action xsi:type="ExecuteFunction">
                  <FunctionName>helloWorld</FunctionName>
                </Action>
              </Control>
            </Group>
          </CustomTab>
        </ExtensionPoint>
      </DesktopFormFactor>
    </Host>
  </Hosts>
  
  <Resources>
  <bt:Images>
    <bt:Image id="Icon16" DefaultValue="https://github.com/rgablejr/rgablejr.github.io/assets/icon-16.png" />
    <bt:Image id="Icon32" DefaultValue="https://github.com/rgablejr/rgablejr.github.io/assets/icon-32.png" />
    <bt:Image id="Icon80" DefaultValue="https://github.com/rgablejr/rgablejr.github.io/assets/icon-80.png" />
  </bt:Images>
  
    <bt:Urls>
      <bt:Url id="Commands.Url" DefaultValue="https://github.com/rgablejr/rgablejr.github.io/functions.html" />
    </bt:Urls>

	<bt:ShortStrings>
		<bt:String id="GroupLabel" DefaultValue="GitHub Tools" />
		<bt:String id="ButtonLabel" DefaultValue="Run My Function" />
	  </bt:ShortStrings>
	  
	  <bt:LongStrings>
		<bt:String id="ButtonTip" DefaultValue="Click this to run custom logic from GitHub Pages without a task pane." />
	  </bt:LongStrings>	
    </Resources>
</VersionOverrides>